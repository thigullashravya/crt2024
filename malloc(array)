#include <stdio.h>
#include <stdlib.h>
int main() {
    int *a;
a=(int*)malloc(5*sizeof(int));
//b=(int*)malloc(sizeof(int));
printf("eneter 5 ele");
for(int i=0;i<5;i++)
{
    scanf("%d",(a+i));
}
for(int i=0;i<5;i++)
{
    printf("%d Address =%u\n",*(a+i),(a+i));
    
}
}
OUTPUT:
eneter 5 ele6
